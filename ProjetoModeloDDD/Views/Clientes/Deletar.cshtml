@model ProjetoModeloDDD.ViewModels.ClienteViewModel

@{
    ViewBag.Title = ViewBag.Message;
}

<h2>@ViewBag.Message</h2>

<p>
    <b>Nome completo: </b> @Html.DisplayFor(Model => Model.Nome) @Html.DisplayFor(Model => Model.SobreNome)
</p>

<p>
    <b>E-Mail: </b> @Html.DisplayFor(Model => Model.Email)
</p>

<p>
    <b>Ativo: </b> @Html.DisplayFor(Model => Model.Ativo)
</p>

<div  class="col-md-offset-2 col-md-10">
    <input  type="submit" value="Deletar" class="btn btn-default">
</div>